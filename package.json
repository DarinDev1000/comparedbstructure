{
  "name": "comparedbstructure",
  "version": "0.1.0",
  "description": "A program to compare the structure of two databases and list the differences.",
  "main": "app.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "devtools": "nodemon --inspect -e js,json,html app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --pretty --lib 'dom','es2015','es2017' --types 'node' models/*.ts routes/*.ts interfaces/*.ts ./*.ts",
    "compile": "npm run tsc && node app.js",
    "dev-ts": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run compile\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DarinDev1000/comparedbstructure"
  },
  "keywords": [
    "database",
    "compare",
    "dbcompare",
    "structure",
    "mysql",
    "mariadb"
  ],
  "author": "Darin Crawmer",
  "license": "MIT",
  "homepage": "https://github.com/DarinDev1000/comparedbstructure",
  "dependencies": {
    "bunyan": "^1.8.12",
    "child-process-promise": "^2.2.0",
    "debug": "^2.6.9",
    "dotenv": "^4.0.0",
    "forever": "^0.15.3",
    "htmlparser": "^1.7.7",
    "jsdom": "^9.11.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.3",
    "koa-body": "^2.5.0",
    "koa-bunyan": "^1.0.1",
    "koa-compose": "^3.2.1",
    "koa-compress": "^2.0.0",
    "koa-lusca": "^2.2.0",
    "koa-router": "^7.3.0",
    "koa-views": "^6.1.4",
    "koa-views-render": "0.0.1",
    "koa2-cors": "^2.0.5",
    "markdown-it": "^8.4.0",
    "moment": "^2.20.1",
    "mysql2": "^1.5.1",
    "mz": "^2.7.0",
    "node-gyp": "^3.8.0",
    "randomstring": "^1.1.5",
    "twig": "^1.12.0",
    "typescript": "^3.1.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.14.11",
    "npm-check": "^5.5.2",
    "npm-check-updates": "^2.14.0",
    "supertest": "^3.0.0"
  }
}
