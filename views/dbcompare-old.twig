<body class="starter-template">
  <br>
  Same Tables in DB:
  <div class="container">
    <span style="background-color: {{color}}">{{tablesSameResult}}</span>
    <br>
    <div class="row">
      {% if tableSame == true %} --------------YES--------------- {% endif %}
      {% if tableSame == false %}
      <ol class="col-md-6 mid-pad">
        <h3>Database 1 Tables</h3>
        {% for row in result1 %} {% for col in row %}
        <li>{{col}}</li>
        {% endfor %} {% endfor %}
      </ol>
      <ol class="col-md-6 mid-pad">
        <h3>Database 2 Tables</h3>
        {% for row in result2 %} {% for col in row %}
        <li>{{col}}</li>
        {% endfor %} {% endfor %}
      </ol>
      {% endif %}
    </div>
  </div>
